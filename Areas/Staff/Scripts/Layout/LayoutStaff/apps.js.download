    const dt999 = new DataTransfer(); // Permet de manipuler les fichiers de l'input file
    $("#attachment999").on('change', function(e){
        HoldOn.open({
            theme:"sk-bounce"
        });
        for(var i = 0; i < this.files.length; i++){
            $(".groupUploadMultiple").prepend('<div class="groupUploadMultipleBtnAdd groupUploadMultipleBtnAddImage p-0"><img class="p-0" src='+URL.createObjectURL(this.files.item(i))+' /><div data-name="'+this.files.item(i).name+'" class="groupUploadMultipleDelete"><svg class="w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M4.293 4.293a1 1 0 011.414 0L10 8.586l4.293-4.293a1 1 0 111.414 1.414L11.414 10l4.293 4.293a1 1 0 01-1.414 1.414L10 11.414l-4.293 4.293a1 1 0 01-1.414-1.414L8.586 10 4.293 5.707a1 1 0 010-1.414z" clip-rule="evenodd"></path></svg></div></div>');
        };
        // Ajout des fichiers dans l'objet DataTransfer
        for (let file of this.files) {
            dt999.items.add(file);
        }
        // Mise à jour des fichiers de l'input file après ajout
        this.files = dt999.files;
        // EventListener pour le bouton de suppression créé
        $('.groupUploadMultipleDelete').click(function(){
            let name = $(this).data('name');
            // Supprimer l'affichage du nom de fichier
            $(this).parent().remove();
            for(let i = 0; i < dt999.items.length; i++){
                // Correspondance du fichier et du nom
                if(name === dt999.items[i].getAsFile().name){
                    // Suppression du fichier dans l'objet DataTransfer
                    dt999.items.remove(i);
                    continue;
                }
            }
            // Mise à jour des fichiers de l'input file après suppression
            document.getElementById('attachment999').files = dt999.files;
        });
        setTimeout(() => {
            HoldOn.close();
            return false;
        }, 1000)
    });
}
